<Settings>
  <Sitecore>
    <XConnect>
      <MarketingAutomation>
        <Engine>
          <Services>
            <LighthouseFitness.EventNotificationService>
              <Type>Sitecore.Demo.Fitness.Feature.Automation.Services.EventNotificationService, Sitecore.Demo.Fitness.Feature.Automation</Type>
              <As>Sitecore.Demo.Fitness.Feature.Automation.Services.IEventNotificationService, Sitecore.Demo.Fitness.Feature.Automation</As>
              <LifeTime>Singleton</LifeTime>
              <Options>
                <FirebaseMessagingApiUri>https://fcm.googleapis.com/fcm/send</FirebaseMessagingApiUri>
              </Options>
            </LighthouseFitness.EventNotificationService>
          </Services>
        </Engine>
      </MarketingAutomation>
    </XConnect>
  </Sitecore>
</Settings>